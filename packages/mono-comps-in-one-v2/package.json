{
  "name": "mono-comps-in-one-v2",
  "version": "0.0.1",
  "description": "hel-mono base comps",
  "repository": {
    "type": "git",
    "url": "https://github.com/hel-eco/hel-mono",
    "directory": "packages/mono-comps"
  },
  "license": "MIT",
  "author": {
    "name": "fancyzhong"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./hel": {
      "types": "./src/.hel/entrance/libTypes.ts",
      "import": "./src/.hel/entrance/libTypes.ts"
    }
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./src/index.ts",
        "import": "./lib/index.js"
      },
      "./hel": {
        "types": "./src/.hel/entrance/libTypes.ts",
        "import": "./src/.hel/entrance/libTypes.ts"
      }
    }
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "src/index.ts",
  "files": [
    "dist",
    "lib",
    "es",
    "src",
    "README.md",
    "tsconfig.json"
  ],
  "scripts": {
    "prebuild": "tsc && node ./scripts/replaceAlias.js",
    "start": "node ../../dev/scripts/hel/start.js",
    "build:hel": "node ../../dev/scripts/hel/build.js",
    "build": "npm run prebuild && npm run build:npm",
    "tsc": "tsc",
    "build:npm": "npm run build:es && npm run build:umd && npm run build:umd:min",
    "build:npm2": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "build:commonjs": "rollup -c --environment BUILD_ENV:commonjs",
    "build:es": "rollup -c --environment BUILD_ENV:es",
    "build:umd": "rollup -c --environment BUILD_ENV:development",
    "build:umd:min": "rollup -c --environment BUILD_ENV:production",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@types/react": ">=16.0.0",
    "@types/react-dom": ">=16.0.0",
    "is-test": "workspace:*",
    "react": ">=16.10.2",
    "react-dom": ">=16.10.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "babel-cli": "^6.26.0",
    "rollup": "^2.23.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-uglify": "^6.0.4"
  },
  "peerDependencies": {
    "@types/react": ">=16.0.0",
    "@types/react-dom": ">=16.0.0",
    "react": ">=16.10.2",
    "react-dom": ">=16.10.2"
  },
  "bundleDependencies": false,
  "deprecated": false,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}